    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.13.0",
    "lodash": "^4.17.3",
    "mocha": "^3.1.2",
    "pre-commit": "^1.1.3",
    "redux": "^3.6.0",
    "reselect": "^2.5.4",
    "standard-version": "^4.0.0"
  },
  "dependencies": {
    "babel-runtime": "6.x.x"
