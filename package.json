{
  "name": "@workpop/dux",
  "version": "2.0.1",
  "description": "HOC Redux Functions",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "release": "standard-version",
    "prepublish": "babel ./src --ignore test --out-dir ./dist",
    "lint": "eslint ./src",
    "fix": "eslint ./src --fix",
